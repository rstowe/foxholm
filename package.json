{
    "name": "foxholm",
    "version": "1.0.0",
    "private": true,
    "description": "Foxholm AI Image Tools - Multi-subdomain image processing application",
    "main": "server/index.js",
    "scripts": {
      "start": "npm run build && node server/index.js",
      "build": "webpack --mode production",
      "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
      "dev:server": "nodemon server/index.js",
      "dev:client": "webpack serve --mode development --hot",
      "test": "npm run eslint",
      "eslint": "eslint 'client/src/**/*.{js,jsx}' 'server/**/*.js'",
      "health-check": "node scripts/health-check.js",
      "docker:build": "docker build -t foxholm-app .",
      "docker:run": "docker run -p 3001:3001 --env-file .env foxholm-app",
      "deploy": "make deploy",
      "logs": "make logs",
      "status": "make status"
    },
    "dependencies": {
      "@fastify/cors": "^8.4.0",
      "@fastify/static": "^6.12.0",
      "@reduxjs/toolkit": "^2.8.2",
      "canvas": "^3.1.0",
      "dotenv": "^16.3.1",
      "fastify": "^4.24.3",
      "pngjs": "^7.0.0",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "react-redux": "^8.1.3",
      "redux": "^4.2.1",
      "together-ai": "^0.16.0"
    },
    "devDependencies": {
      "@babel/core": "^7.23.5",
      "@babel/eslint-parser": "^7.27.5",
      "@babel/preset-env": "^7.23.5",
      "@babel/preset-react": "^7.23.3",
      "@eslint/js": "^9.28.0",
      "babel-loader": "^9.1.3",
      "concurrently": "^9.1.2",
      "copy-webpack-plugin": "^11.0.0",
      "css-loader": "^6.8.1",
      "eslint": "^9.28.0",
      "eslint-plugin-react": "^7.37.5",
      "eslint-plugin-react-hooks": "^5.2.0",
      "globals": "^16.2.0",
      "html-webpack-plugin": "^5.5.3",
      "nodemon": "^3.0.2",
      "style-loader": "^3.3.3",
      "webpack": "^5.89.0",
      "webpack-cli": "^5.1.4",
      "webpack-dev-server": "^5.2.2"
    },
    "engines": {
      "node": ">=22.0.0"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/yourusername/foxholm.git"
    },
    "keywords": [
      "ai",
      "image-processing",
      "headshot",
      "photo-restoration",
      "image-upscaling",
      "together-ai",
      "fastify",
      "react"
    ],
    "author": "Your Name",
    "license": "UNLICENSED"
  }